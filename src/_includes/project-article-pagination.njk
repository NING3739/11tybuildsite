<div class="article-pagination">
  {% set previousPost = collections.project | getPreviousCollectionItem(page) %}
  {% set nextPost = collections.project | getNextCollectionItem(page) %}
  {% if previousPost %}
    <a href="{{ previousPost.url }}">Previous:&nbsp;{{ previousPost.data.title }}</a>
  {% endif %}
  <br>
    {% if nextPost %}
      <a href="{{ nextPost.url }}">
        Next:&nbsp;{{ nextPost.data.title }}</a>
    {% endif %}
  </div>